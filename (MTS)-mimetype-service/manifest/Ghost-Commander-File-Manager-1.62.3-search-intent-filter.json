{
    "root": {
        "@xmlns:ns0": "http://schemas.android.com/apk/res/android",
        "intent-filter": [
            {
                "action": {
                    "@ns0:name": "android.intent.action.MAIN"
                },
                "category": [
                    {
                        "@ns0:name": "android.intent.category.LAUNCHER"
                    },
                    {
                        "@ns0:name": "android.intent.category.MULTIWINDOW_LAUNCHER"
                    }
                ]
            },
            {
                "action": {
                    "@ns0:name": "android.intent.action.PICK"
                },
                "data": {
                    "@ns0:mimeType": "*/*"
                },
                "category": {
                    "@ns0:name": "android.intent.category.DEFAULT"
                }
            },
            {
                "action": {
                    "@ns0:name": "android.intent.action.GET_CONTENT"
                },
                "data": {
                    "@ns0:mimeType": "*/*"
                },
                "category": [
                    {
                        "@ns0:name": "android.intent.category.DEFAULT"
                    },
                    {
                        "@ns0:name": "android.intent.category.OPENABLE"
                    }
                ]
            },
            {
                "action": {
                    "@ns0:name": "android.intent.action.VIEW"
                },
                "category": [
                    {
                        "@ns0:name": "android.intent.category.DEFAULT"
                    },
                    {
                        "@ns0:name": "android.intent.category.BROWSABLE"
                    }
                ],
                "data": [
                    {
                        "@ns0:scheme": "ftp"
                    },
                    {
                        "@ns0:scheme": "file"
                    }
                ]
            },
            {
                "action": {
                    "@ns0:name": "android.intent.action.VIEW"
                },
                "category": [
                    {
                        "@ns0:name": "android.intent.category.DEFAULT"
                    },
                    {
                        "@ns0:name": "android.intent.category.BROWSABLE"
                    }
                ],
                "data": [
                    {
                        "@ns0:scheme": "file"
                    },
                    {
                        "@ns0:scheme": "content"
                    },
                    {
                        "@ns0:mimeType": "inode/directory"
                    }
                ]
            },
            {
                "action": {
                    "@ns0:name": "android.intent.action.VIEW"
                },
                "category": [
                    {
                        "@ns0:name": "android.intent.category.DEFAULT"
                    },
                    {
                        "@ns0:name": "android.intent.category.BROWSABLE"
                    }
                ],
                "data": [
                    {
                        "@ns0:mimeType": "application/x-zip-compressed"
                    },
                    {
                        "@ns0:mimeType": "application/zip"
                    },
                    {
                        "@ns0:pathPattern": ".*.zip"
                    }
                ]
            },
            {
                "action": {
                    "@ns0:name": "android.intent.action.SEARCH_LONG_PRESS"
                },
                "category": {
                    "@ns0:name": "android.intent.category.DEFAULT"
                }
            },
            {
                "action": {
                    "@ns0:name": "android.intent.action.VIEW"
                },
                "category": {
                    "@ns0:name": "android.intent.category.DEFAULT"
                },
                "data": {
                    "@ns0:mimeType": "text/*"
                }
            },
            {
                "action": [
                    {
                        "@ns0:name": "android.intent.action.EDIT"
                    },
                    {
                        "@ns0:name": "android.intent.action.VIEW"
                    }
                ],
                "category": {
                    "@ns0:name": "android.intent.category.DEFAULT"
                },
                "data": {
                    "@ns0:mimeType": "text/*"
                }
            },
            {
                "action": {
                    "@ns0:name": "android.intent.action.VIEW"
                },
                "category": {
                    "@ns0:name": "android.intent.category.DEFAULT"
                },
                "data": {
                    "@ns0:mimeType": "image/*"
                }
            }
        ]
    }
}