                "category": {
                    "@ns0:name": "android.intent.category.DEFAULT"
                }
            },
            {
                "action": {
                    "@ns0:name": "android.intent.action.SEND"
                },
                "category": {
                    "@ns0:name": "android.intent.category.DEFAULT"
