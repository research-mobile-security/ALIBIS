                    "@ns0:mimeType": "image/*"
                }
            },
            {
                "action": [
                    {
                        "@ns0:name": "android.intent.action.MEDIA_EJECT"
                    },
                    {
                        "@ns0:name": "android.intent.action.MEDIA_MOUNTED"
                    }
